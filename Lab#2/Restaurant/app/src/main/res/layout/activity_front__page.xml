<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Front_Page">

    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" app:srcCompat="@drawable/prueba"
            android:id="@+id/chuckle"
            android:contentDescription="@string/Restaurant_Icon"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="104dp" android:layout_marginRight="104dp" android:layout_marginTop="48dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="10dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="10dp"
            app:layout_constraintHorizontal_bias="0.891"/>
    <EditText
            android:layout_width="211dp"
            android:layout_height="44dp"
            android:inputType="textPersonName"
            android:text="@android"
            android:ems="10"
            android:id="@+id/chucklefish" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="84dp" android:layout_marginRight="84dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="85dp"
            android:layout_marginStart="85dp" android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/chuckle"/>

</android.support.constraint.ConstraintLayout>